{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "es2015",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "strict": false,
    "target": "ES6"
  },
  "exclude": ["node_modules"],
  "files": ["src/vue-shims.d.ts"],
  "include": ["src/**/*.ts", "src/**/*.vue","src/static/images/*.*"]
}
